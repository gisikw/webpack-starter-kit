{
  "name": "webpack-starter-kit",
  "version": "0.1.0",
  "description": "A webpack template app configured for tests, coverage, and production releases",
  "scripts": {
    "test:watch": "WATCH=true node ./node_modules/.bin/karma start",
    "test": "node ./node_modules/.bin/karma start",
    "start": "DEV_SERVER=true node webpack.config.js",
    "build": "node ./node_modules/.bin/webpack -p"
  },
  "author": "Kevin Gisi <kevin@kevingisi.com> (http://twitter.com/cheerskevin)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "core-js": "^1.1.1",
    "html-webpack-plugin": "^1.6.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
